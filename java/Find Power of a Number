
import java.util.Scanner; import java.lang.Math;

public class Solution {

public static void main(String[] args) {
    // Write your code here
    int x,n;
    double e=0.0;
    Scanner s = new Scanner(System.in);
    x=s.nextInt();
    n=s.nextInt();
    e = Math.pow(x, n);
    System.out.println((int)e);
}
}
