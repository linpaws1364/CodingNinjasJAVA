import java.util.Scanner;

public class Main{

public static void main(String[] args) {
	// Write your code here
    int i,o=0,e=0,N,count;
    Scanner s = new Scanner(System.in);
    N=s.nextInt();
    while (N!= 0)
    {
        i = N % 10;
        if (i%2 == 0)
            e+=i;
        if (i%2 != 0)
            o+=i;
        N=(int)N/10;
    }
    System.out.println(e+ " "+ o);
}
}
